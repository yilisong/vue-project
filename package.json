{
  "name": "daoshu",
  "version": "1.0.0",
  "description": "daoshu manager 1.0",
  "author": "yilisong <yils@daoshu.com>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yilisong/vue_webpack_test.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "clean:all": "rm -rf ./node_modules && rm -rf ./dist && npm run clean",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll && npm run bootstrap",
    "npmcheckversion": "node ./packages/dev-utils/npmcheckversion.js",
    "dev": "cd ./packages/server && npm run dev",
    "analyze": "cd ./packages/server && npm run analyze",
    "local": "cd ./packages/server && npm run local",
    "stage": "cd ./packages/server && npm run stage",
    "pre": "cd ./packages/server && npm run pre",
    "release": "cd ./packages/server && npm run release",
    "build": "cross-env NODE_ENV=production node ./scripts/build.js -x md4",
    "build:release": "cross-env NODE_ENV=production APP_ENV=release node ./scripts/build.js -x md4",
    "build:dll": "node ./scripts/dependencies.js",
    "bundle": "sh ./tasks/bundle.sh",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "concurrent": "false",
    "linters": {
      "+(src|config|packages|scripts)/**/*.js": [
        "prettier --jsx-bracket-same-line --single-quote --no-semi --write",
        "git add"
      ],
      "*.{js,vue}": "eslint --quiet --format node_modules/eslint-friendly-formatter"
    }
  },
  "dependencies": {
    "axios": "^0.16.0",
    "core-js": "^2.4.1",
    "echarts": "^3.4.0",
    "fullpage.js": "^2.9.4",
    "gsap": "1.20.3",
    "iview": "^2.7.4",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "normalize.css": "^5.0.0",
    "nprogress": "^0.2.0",
    "qs": "^6.4.0",
    "swiper": "^3.4.2",
    "tween.js": "^16.6.0",
    "vue": "2.3.0",
    "vue-dynamic-props": "0.0.4",
    "vue-meta": "^1.0.5",
    "vue-router": "^2.3.0",
    "vuex": "^2.2.1",
    "vuex-persistedstate": "^1.4.1",
    "whatwg-fetch": "0.11.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.4.2",
    "babel-preset-env": "^1.3.3",
    "babel-preset-flow": "^6.23.0",
    "cheerio": "^0.22.0",
    "circular-dependency-plugin": "^3.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "eslint": "^3.19.0",
    "eslint-config-yundun-fe": "0.0.3",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^0.10.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.13.1",
    "json-loader": "^0.5.4",
    "lerna": "2.4.0",
    "lint-staged": "^3.5.1",
    "ora": "^1.3.0",
    "postcss-import": "^9.1.0",
    "postcss-inject": "^1.0.1",
    "postcss-loader": "^1.3.3",
    "postcss-scss": "^0.4.1",
    "precss": "^1.4.0",
    "prettier": "^1.3.1",
    "prettier-eslint": "^6.1.0",
    "rimraf": "^2.6.1",
    "rucksack-css": "^0.9.1",
    "shelljs": "^0.7.5",
    "style-loader": "^0.13.2",
    "three": "^0.86.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^11.3.3",
    "vue-style-loader": "^2.0.3",
    "vue-template-compiler": "2.3.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  }
}
